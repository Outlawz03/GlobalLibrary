        <Script Name="bindManager" OnDocumentLoad="load"><![CDATA[global.bindManager = Object.create(global.helpers);

global.bindManager.change = function(e) {
	var target = e.getTarget();
	
	var targetName = String(target.getAttribute("Name"));
	var parent = target.getParentNode();		
	while(String(parent.getTagName()) !== "Tgml") {
		var parentName = String(parent.getAttribute("Name"));		
		if(parentName.toLowerCase().trim() !== "" && parentName.toLowerCase().trim() !== "binds" && parentName.toLowerCase().trim() !== "view") {
			targetName = parentName + "_" + targetName;
		}	
		parent = parent.getParentNode();
	}
		
	if(!this.hasOwnProperty(targetName)) {
		this[targetName] = Object.create(global.bind);
		this[targetName].init(target);
	}
};

// mandatory load event
function load(evt) {
}
]]></Script>
        <Script Name="main" OnDocumentLoad="load"><![CDATA[global.main = Object.create(global.helpers);

global.main.load = function(e) {
	global.doc = e.getCurrentTarget().getOwnerDocument();
	global.tgml = global.doc.getDocumentElement();
	var comp = e.getCurrentTarget();
	var	children = this.getChildrenByAttribute(comp, "Ctrl"); // list of all components with the attribute Ctrl
	
	// create a new model for each control found
	this.models = {};
	for(var i = 0, max = children.length; i < max; i += 1) {
		var child = children[i];
		var	ctrl = String(child.getAttribute("Ctrl"));
		
		// build Model name
		var modelName = String(child.getAttribute("Name"));
		var parent = child.getParentNode();		
		while(String(parent.getTagName()) !== "Tgml") {
			var parentName = String(parent.getAttribute("Name"));
			var parentId = String(parent.getAttribute("Id"));
			if(parentName.toLowerCase().trim() !== "") {
				modelName = parentName + "_" + modelName;
			} else if(parentId.toLowerCase().trim() !== "") {
				modelName = parentId + "_" + modelName;
			}	
			parent = parent.getParentNode();
		}
		
		// try to add new model if controller exists
		try {
			this.models[modelName] = Object.create(eval("global." + ctrl));
			this.models[modelName].init(child);							
		} catch(err) {
			alert(err + "\n\n Programmer Note: "+ ctrl +" does not exist in the Script Library. @" + modelName);
		}
	}
	
	
	global.bm = Object.create(global.bindManager); 
};
	
global.main.change = function(e) {
	e.preventDefault();
	global.bm.change(e);
	var attbList = e.getTarget().attributes;
		
	for(var i = 0, max = attbList.length; i < max; i += 1) {
		var attb = attbList.item(i).nodeName;
		var attbPath = attb;
		var parent = e.getTarget().getParentNode();
		while(String(parent.getTagName()) !== "Tgml") {
			var parentName = String(parent.getAttribute("Name"));
			var parentId = String(parent.getAttribute("Id"));
			if(parentName.toLowerCase().trim() !== "binds") {
				if (parentName.toLowerCase().trim() !== "") {
					attbPath = parentName + "_" + attbPath;
				} else if(parentId.toLowerCase().trim() !== "") {
					attbPath = parentId + "_" + attbPath;
				}
			}
			parent = parent.getParentNode();
		}
		if(this.models.hasOwnProperty(attbPath)) {
			this.models[attbPath].change(e);
		} else if(this.models.hasOwnProperty(attb)){
			this.models[attb].change(e);
		}
	}
};

global.main.mouse = function(e, action) {
	var attbList = e.getTarget().attributes;
		
	for(var i = 0, max = attbList.length; i < max; i += 1) {		
		var attb = String(attbList.item(i).nodeName).split("_").slice(0, -1).join("_");
		var event = String(attbList.item(i).nodeName).split("_").pop();
		var attbPath = attb;
		var parent = e.getTarget().getParentNode();
		while(String(parent.getTagName()) !== "Tgml") {
			var parentName = String(parent.getAttribute("Name"));
			var parentId = String(parent.getAttribute("Id"));
			if(parentName.toLowerCase().trim() !== "binds") {
				if (parentName.toLowerCase().trim() !== "") {
					attbPath = parentName + "_" + attbPath;
				} else if(parentId.toLowerCase().trim() !== "") {
					attbPath = parentId + "_" + attbPath;
				}
			}
			parent = parent.getParentNode();
		}
		
		if(event === action) {				
			if(this.models.hasOwnProperty(attbPath)){
				if(!this.isUndefined(this.models[attbPath][action])) {
					this.models[attbPath][action](e);
				}
			} else if(this.models.hasOwnProperty(attb)){
				if(!this.isUndefined(this.models[attb][action])) {
					this.models[attb][action](e);
				}
			}
		}
	}
};

// mandatory load event
function load(evt) {
}
]]></Script>
