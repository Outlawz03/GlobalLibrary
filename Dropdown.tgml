        <Script Name="baseValue" OnDocumentLoad="load"><![CDATA[global.baseValue = Object.create(global.baseNode);

global.baseValue.click = function(e) {
	if(this.node.hasAttribute("RemoteBind")) {
		if(this.getBool(this.node.getAttribute("RemoteBind"))) {
			if(Number(e.getButton()) === 0) {
				e.preventDefault();
			}
		}
	}
}

global.baseValue.up = function(e) {
	if(this.node.hasAttribute("RemoteBind")) {
		if(this.getBool(this.node.getAttribute("RemoteBind"))) {
			var target = e.getTarget();
			var targetName = String(target.getAttribute("Name"));
			
			var parent = target.getParentNode();		
			while(String(parent.getTagName()) !== "Tgml") {
				var parentName = String(parent.getAttribute("Name"));
				if(parentName.toLowerCase().trim() !== "" && parentName.toLowerCase().trim() !== "binds" && parentName.toLowerCase().trim() !== "view") {
					targetName = parentName + "_" + targetName;
				} 
				parent = parent.getParentNode();
			}

			if(Number(e.getButton()) === 0) {
				if(global.bm.hasOwnProperty(targetName)) {
					global.bm[targetName].displayProperties();
				}
			}
		}
	}
}

// mandatory load event
function load(evt) {
