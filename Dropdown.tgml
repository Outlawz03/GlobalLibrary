            <Script Name="groupObject" OnDocumentLoad="load"><![CDATA[global.groupObject = Object.create(global.baseNode);

global.groupObject.getParameters = function() { 		
 	// get a list of binds
 	var bindList = this.node.getElementsByTagName("Bind");
 	this.binds = [];
 	for(var i = 0, max = bindList.length; i < max; i += 1) {
 		var child = bindList.item(i);
 		this.binds.push(child);
 	}
 		
 	// create next pointer
 	this.next = null;
 		
 	// store initial height
 	this.height = +this.node.getAttribute("Height");
 	this.width = +this.node.getAttribute("Width");
 		
 	// get rows, arrow, and background rectangle
 	this.rowsGroup = this.node.getChild("rows");
 	this.arrow = this.node.getChild("arrow");
 	this.arrow.setAttribute("Left", this.width - 30);
 	this.bgd = this.node.getChild("bgd");
 		
 	// get header height
 	this.headerHeight = +this.node.getChild("header").getAttribute("Height");
 	var arrowHeight = +this.arrow.getAttribute("Height");
 	this.arrow.setAttribute("Top", (this.headerHeight - arrowHeight) / 2);
 	this.title = this.node.getChild("title");
 	this.title.setAttribute("Top", (this.headerHeight - 20) / 2);
 		
 	// get initial active status
 	this.active = this.getBool(this.node.getAttribute("initiallyExpanded"));
 		
 	// get row objects
 	this.rows = {};
 	var children = this.rowsGroup.getChildNodes();
 	this.rowsHead = null;
 	this.rowsTail = null;
 		
 	for(var i = 0, max = children.length; i < max; i += 1) {
 		var child = children.item(i);
 		var name = String(child.getAttribute("Id"));
		var tagName = String(child.getTagName());
 		if(tagName === "Component") {
 			this.rows[name] = Object.create(global.rowObject);
 			this.rows[name].init(child); 				
 				
 			// first loop only set up the head and the tail
 			if(this.rowsHead === null) {
 				// set the name of the head
 				this.rowsHead = name;
 				// tie the tail to the head
 				this.rowsTail = this.rowsHead;
 			}
 				
 			// make the tails next equal the current object
 			this.rows[this.rowsTail].next = name;
 			// set the tail to the object
 			this.rowsTail = name;
 		}
 	}
 	
 	if(this.rows.length = 1) {
 		this.rows[this.rowsTail].next = null;
 	}
 		
 	// make bottom line invisible
 	if(this.rows.hasOwnProperty(this.rowsTail)) {
 		this.rows[this.rowsTail].node.getChildRecursive("Hline").setAttribute("Visibility", "Hidden");
 	}
 		
 	this.stack();
 	this.setBinds();
};
 	
global.groupObject.toggleActiveStatus = function() {
 	this.active = !this.active;
 	this.stack();
 	this.setBinds();
};
 		
global.groupObject.stack = function() {
 	// store the name of the head
 	var name = this.rowsHead;
 
 	// start at header bottom
 	var top = this.headerHeight;
 		
 	// loop through each group object
 	while(name !== null) {
 		// set the top property
 		this.rows[name].node.setAttribute("Top", top);
 			
 		// add the height and buffer to top
 		top += this.rows[name].height;
 			
 		// advance the name to the next object
 		name = this.rows[name].next;
 	}
 		
 	// store new height
 	this.height = this.active ? top : this.headerHeight;
 		
 	// set bgd height
 	this.bgd.setAttribute("Height", this.height);
};
 	
global.groupObject.setBinds = function() {
 	for(var i = 0, max = this.binds.length; i < max; i += 1) {
 		try {
			this.binds[i].setAttribute("DynamicUpdates", this.active ? "Enable" : "Disable");
		} catch(err) {
		}
	}
};

// mandatory load event
function load(evt) {
}
]]></Script>
