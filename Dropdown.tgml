        <Script Name="title" OnDocumentLoad="load"><![CDATA[global.title = Object.create(global.baseNode);

global.title.getParameters = function() {
	var position = "";
	if(this.node.hasAttribute("TitlePosition")) {		
		position = String(this.node.getAttribute("TitlePosition"));
	}
	
	var content = String(this.node.getAttribute("Content"));
	if(content === "") {
		content = String(this.node.getParentNode().getAttribute("Id"));
	}
	var width = Number(this.node.getAttribute("Width"));
	var left = width / 2;
	var top = -20.0;
	var horizontalAlign = "Center";

	switch(position.toLowerCase().trim()) {
	case "left": case "l":
		left = -20.0;
		top = 0.0;
		horizontalAlign = "Right";
		break;
	case "right": case "r":
		left = width + 20.0;
		top = 0.0;
		horizontalAlign = "Left";
		break;
	default:		
	}
	
	this.render({Content: content, Left: left, Top: top, HorizontalAlign: horizontalAlign});
};

// mandatory load event
function load(evt) {
}
]]></Script>
        <Group Name="indications">
            <Script Name="indication" OnDocumentLoad="load"><![CDATA[global.indication = Object.create(global.baseNode);

global.indication.getParameters = function() {
	this.values = {};
};

global.indication.storeValue = function(e) {
	var target = e.getTarget();
	var targetName = target.getAttribute("Name");
	var targetDscr = String(target.getAttribute("Description"));		
	
	if(!this.values.hasOwnProperty(targetName)) {
		this.values[targetName] = {value: e.getValue(), dscr: targetDscr, status: Number(e.getStatus())};
	}
		
	this.values[targetName].value = e.getValue();		
	this.values[targetName].status = Number(e.getStatus());
};

global.indication.isForced = function() {
	for(key in this.values) {
		var value = this.values[key];
		if(value.dscr === "Value") {				
			if(value.status === 3) {
				return true;
			}
		}
	}
	
	return false;
};

global.indication.isBacnetForced = function() {
	for(key in this.values) {
		var value = this.values[key];
		if(value.dscr === "ForceIndication") {
			var lastName = String(String(key).split(".").splice(-1)).replace(/\d/g,'');
 		 		
			switch(lastName) {
			case "OutOfService":
				if(this.getBool(value.value)) {
					return true;
				}		
				break;
			case "Priority":
				if(value.value !== null && value.value !== "") {
					return true;
				}
				if(typeof(value.value) === "undefined") {
					return false;
				}
				break;
			case "ActiveCommandPriority":
				if(value.value !== null && value.value !== "" && value.value < 10) {
					return true;
				}
				break;
			default:
			}
		}
	}	
	return false;
};

global.indication.isInAlarm = function() {
	for(key in this.values) {
		var value = this.values[key];
		switch(value.dscr) {
		case "Alarm":
			if(this.getBool(value.value)) {
				return true;
			}
		}
	}
	
	return false;
};

// mandatory load event
function load(evt) {
}
]]></Script>
