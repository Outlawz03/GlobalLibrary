        <Script Name="matrix" OnDocumentLoad="load"><![CDATA[global.matrix = Object.create(global.baseNode);

global.matrix.getParameters = function() {
	this.groups = {};
 	var children = this.node.getChildNodes();
 	this.groupsHead = null;
 	this.groupsTail = null;

 	for(var i = 0; i < children.length; i += 1) {
 		var child = children.item(i);
 		var name = String(child.getAttribute("Id")); 	
 		if(String(child.getTagName()) === "Component") { 		 	
 			this.groups[name] = Object.create(global.groupObject);
 			this.groups[name].init(child);  				
 	
 			// first loop only set up the head and the tail
 			if(this.groupsHead === null) {
 				// set the name of the head
 				this.groupsHead = name;
 				// tie the tail to the head
 				this.groupsTail = this.groupsHead;
 			}
 				
 			// make the tails next equal the current object
 			this.groups[this.groupsTail].next = name;
 			// set the tail to the object
			this.groupsTail = name;
		}
 	}
 	
 	// cut off the linked list
 	if(this.groupsTail !== null) {
 		this.groups[this.groupsTail].next = null;
 	}
 		
 	// get the buffer between the groups
 	this.groupBuffer = Number(this.node.getAttribute("GroupBuffer"));
 		
 	// stack the groups
 	this.stack();
};

global.matrix.up = function(e) {
 	var target = e.getTarget();
	var targetName = String(target.getAttribute("Name"));
	var parentId = String(target.getParentNode().getAttribute("Id"));
 	
 	// if a group was clicked then toggle its active status
 	if(Number(e.getButton()) === 0) {
		if(this.groups.hasOwnProperty(targetName)) {
 			this.groups[targetName].toggleActiveStatus();
		} else if(this.groups.hasOwnProperty(parentId)) {
			this.groups[parentId].toggleActiveStatus();
		}
	}
	
	// restack the groups
	this.stack();
};

global.matrix.stack = function() {
 	// store the name of the head
 	var name = this.groupsHead;
 
 	// start at zero
 	var top = 0;
 		
 	// loop through each group object
 	while(name !== null) {
 		// set the top property
 		this.groups[name].node.setAttribute("Top", top);
 			
 		// add the height and buffer to top
 		top += this.groups[name].height + this.groupBuffer;
 			
 		// advance the name to the next object
 		name = this.groups[name].next;
	}
};

global.matrix.click = function(e) {
	e.preventDefault();
}

// mandatory load event
function load(evt) {
}
]]></Script>
