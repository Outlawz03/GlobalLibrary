	var i = 2;
	while(i--) {
		this.states[i] = String(this.node.getAttribute("State" + i));
	}
};
	
global.digitalValue.change = function(e) {
	var value = e.getValue();
	
	var content = this.getBool(value) ? this.states[1] : this.states[0]
	this.render({Content: content});
};

// mandatory load event
function load(evt) {
}
]]></Script>
            <Script Name="multistateValue" OnDocumentLoad="load"><![CDATA[global.multistateValue = Object.create(global.baseValue);

global.multistateValue.getParameters = function() {
	this.states = [];	
				
	var i = this.node.hasAttribute("State-1") ? -1 : this.node.hasAttribute("State0") ? 0 : 1;
	while(this.node.hasAttribute("State" + i)) {
		this.states[i] = String(this.node.getAttribute("State" + i));
		i += 1;
	}
};
	
global.multistateValue.change = function(e) {
	var value = Number(e.getValue());
	
	var content = this.states[value];
	this.render({Content: content});
};

// mandatory load event
function load(evt) {
}
]]></Script>
