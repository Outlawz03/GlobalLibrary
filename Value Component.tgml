            <Script Name="valueBackground" OnDocumentLoad="load"><![CDATA[global.valueBackground = Object.create(global.indication);

global.valueBackground.getParameters = function() {
	this.normalStroke = this.node.getAttribute("Stroke");
	this.normalStrokeWidth = this.node.getAttribute("StrokeWidth");
	this.forcedStroke = "#FFD100";
	this.forcedStrokeWidth = 2;
	this.alarmStroke = "#F0080E";
	this.alarmStrokeWidth = 2;
	this.values = {};
	
	if(this.node.hasAttribute("Color")) {
		this.forcedStroke = this.node.getAttribute("Color");
	}
	if(this.node.hasAttribute("ForcedStrokeWidth")) {
		this.forcedStrokeWidth = this.node.getAttribute("ForcedStrokeWidth");
	}
	if(this.node.hasAttribute("Background")) {
		this.alarmStroke = this.node.getAttribute("Background");
	}
	if(this.node.hasAttribute("AlarmStrokeWidth")) {
		this.alarmStrokeWidth = this.node.getAttribute("AlarmStrokeWidth");
	}
};
	
global.valueBackground.change = function(e) {
	this.storeValue(e);
	
	var stroke = this.normalStroke;
	var strokeWidth = this.normalStrokeWidth;

	if(this.isForced() || this.isBacnetForced()) {
		stroke = this.forcedStroke;
		strokeWidth = this.forcedStrokeWidth;
	} else if(this.isInAlarm()) {
		stroke = this.alarmStroke;
		strokeWidth = this.alarmStrokeWidth;
	}

	this.render({Stroke: stroke, StrokeWidth: strokeWidth});	
};

// mandatory load event
function load(evt) {
}
]]></Script>
