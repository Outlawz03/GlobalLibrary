        <Script Name="baseValue" OnDocumentLoad="load"><![CDATA[global.baseValue = Object.create(global.baseNode);

global.baseValue.click = function(e) {
	if(this.node.hasAttribute("RemoteBind")) {
		if(this.getBool(this.node.getAttribute("RemoteBind"))) {
			if(Number(e.getButton()) === 0) {
				e.preventDefault();
			}
		}
	}
}

global.baseValue.up = function(e) {
	if(this.node.hasAttribute("RemoteBind")) {
		if(this.getBool(this.node.getAttribute("RemoteBind"))) {
			var target = e.getTarget();
			var targetName = String(target.getAttribute("Name"));
			
			var parent = target.getParentNode();		
			while(String(parent.getTagName()) !== "Tgml") {
				var parentName = String(parent.getAttribute("Name"));
				if(parentName.toLowerCase().trim() !== "" && parentName.toLowerCase().trim() !== "binds" && parentName.toLowerCase().trim() !== "view") {
					targetName = parentName + "_" + targetName;
				} 
				parent = parent.getParentNode();
			}

			if(Number(e.getButton()) === 0) {
				if(global.bm.hasOwnProperty(targetName)) {
					global.bm[targetName].displayProperties();
				}
			}
		}
	}
}

// mandatory load event
function load(evt) {
}
]]></Script>
        <Group Name="values">
            <Script Name="analogValue" OnDocumentLoad="load"><![CDATA[global.analogValue = Object.create(global.baseValue);

global.analogValue.getParameters = function() {
	this.decimals = 0;
	this.prefix = "";
	this.suffix = "";
	this.showUnits = true;
		
	if(this.node.hasAttribute("Decimals")) {		
		this.decimals = Number(this.node.getAttribute("Decimals"));
	}
					
	if(this.node.hasAttribute("Prefix")) {		
		this.prefix = String(this.node.getAttribute("Prefix"));
	}
		
	if(this.node.hasAttribute("Suffix")) {		
		this.suffix = String(this.node.getAttribute("Suffix"));
	}
		
	if(this.node.hasAttribute("ShowUnits")) {		
		this.showUnits = this.getBool(this.node.getAttribute("ShowUnits"));
	}
};
	
global.analogValue.change = function(e) {
	var value = Number(e.getValue());
	var units = String(e.getUnit());
	if(units === "null") {
		units = "";
	}
	var content = this.prefix + value.toFixed(this.decimals) + this.suffix + (this.showUnits ? " " + units : "")
	this.render({Content: content});	
};

// mandatory load event
function load(evt) {
}
]]></Script>
            <Script Name="digitalValue" OnDocumentLoad="load"><![CDATA[global.digitalValue = Object.create(global.baseValue);

global.digitalValue.getParameters = function() {
